<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>CustoRegisterServlet</servlet-name>
        <servlet-class>com.spacev.ArchitectFirmBackEnd.servlets.CustoRegisterServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>CustoLoginServlet</servlet-name>
        <servlet-class>com.spacev.ArchitectFirmBackEnd.servlets.CustoLoginServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>CustomerLogoutServlet</servlet-name>
        <servlet-class>com.spacev.ArchitectFirmBackEnd.servlets.CustomerLogoutServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>AddProjectServlet</servlet-name>
        <servlet-class>com.spacev.ArchitectFirmBackEnd.servlets.AddProjectServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CustoRegisterServlet</servlet-name>
        <url-pattern>/custo-register</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CustoLoginServlet</servlet-name>
        <url-pattern>/custo-login</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>CustomerLogoutServlet</servlet-name>
        <url-pattern>/customer-logout-servlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>AddProjectServlet</servlet-name>
        <url-pattern>/add_project</url-pattern>
    </servlet-mapping>

</web-app>